                            <rdg wit="#K-Ib" xml:id="rdg.93">Szép, csendes</rdg>
                            <rdg wit="#K-Id #K5j" xml:id="rdg.94"> szép, tsendes</rdg>
                            <rdg wit="#K-II #K-IIa #K-IIc" xml:id="rdg.95"><add><note type="critic"
