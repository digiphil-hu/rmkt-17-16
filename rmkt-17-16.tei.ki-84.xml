    <text>
        <body>
            <div type="poem" xml:id="d.1" xml:lang="hu">
                <fw type="originalNum">[RMKT XVII/16:] 84.</fw>
                <head type="title">REGGELI IMÁDSÁG</head>
                <lg n="1" xml:id="lg.1">
                    <l n="1" xml:id="l.1">Engemet jó kedvel, Istenem, álmombúl</l>
                    <l n="2" xml:id="l.2">Hogyfel serkentettél nagy bő irgalmadbúl,</l>
                    <l n="3" xml:id="l.3">Ébren emeltél fel éltem unalmábúl,</l>
                    <l n="4" xml:id="l.4">El nem hagytál mostis kegyes jó-vóltodbúl,</l>
                </lg>
                <lg n="2" xml:id="lg.2">
                    <l n="5" xml:id="l.5">Adok azért hálát te szent Főlségednek,</l>
                    <l n="6" xml:id="l.6">Irgalmas vóltodért nagy kegyességednek,</l>
                    <l n="7" xml:id="l.7">Hozzám meg-mutatot ingyen kegyelmednek,</l>
                    <l n="8" xml:id="l.8">Őrők dűcsőssége légyen szent nevednek.</l>
                </lg>
            </div>
        </body>
    </text>
